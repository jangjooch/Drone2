<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="exam03.MainController">
   <bottom>
      <HBox alignment="CENTER" spacing="10.0" style="-fx-background-color: #cccccc;" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="Hbox_Btn_Ok" minHeight="40.0" minWidth="100.0" mnemonicParsing="false" text="OK" />
            <Button fx:id="Hbox_Btn_Cancel" minHeight="40.0" minWidth="100.0" mnemonicParsing="false" text="Cancel" />
            <Button fx:id="Hbox_Btn_Print" minHeight="40.0" minWidth="100.0" mnemonicParsing="false" text="Print" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
